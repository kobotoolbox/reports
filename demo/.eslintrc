{
  "plugins": [
    "react"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true,
  },
  "env": {
    "browser": true,
    "amd": true,
    "es6": true
  },
  "rules": {
    "quotes": [ 1, "single" ],
    "no-undef": 0,
    "no-console": 0,
    "camelcase": 0,
    "global-strict": 0,
    "no-extra-semi": 1,
    "comma-dangle": 0,
    "no-underscore-dangle": 0
  }
}
